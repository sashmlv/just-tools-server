    "eslint-plugin-unicorn": "^29.0.0",
    "eslint-plugin-xss": "^0.1.10",
    "http-server": "^0.12.3",
    "jasmine": "^3.7.0",
    "jasmine-console-reporter": "^3.1.0",
    "jsdoc": "^3.6.6",
